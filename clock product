#include <stdio.h>
int clockProduct(int x,int y)
{
    int product=x*y;
    int result=product%12;
    if(result==0){
        return 12;
    }
    else {
        return result;
        
    }
}
    int main()
    {
    int x=4;
    int y=3;
  
     int output = clockProduct(x, y);
    printf("The clock product of %d and %d is: %d\n", x, y, output);
     return 0;
}
